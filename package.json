    "typescript": "~2.4.2"
  },
  "config": {
    "ionic_aot_write_to_disk": true,
    "ionic_source_map": "source_map"
  },
  "cordova": {
    "platforms": [],
    "plugins": {
      "cordova-plugin-crosswalk-webview": {},
      "cordova-plugin-device": {},
      "cordova-plugin-inappbrowser": {},
